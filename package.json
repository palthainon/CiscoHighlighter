{
	"name": "CiscoHighlighter",
	"displayName": "Cisco IOS Highlighter",
	"description": "Cisco IOS Highlighter by Derick Lawson",
	"version": "1.0.3",
	"publisher": "DerickL",
	"icon": "images/logo.png",
	"galleryBanner": {
		"color": "#000000",
		"theme": "dark"
	},
	"galleryImages": [
		"images/highlight-preview.png"
	],
	"preview": true,
	"license": "Apache-2.0",
	"engines": {
		"vscode": "^1.5.0"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^18.19.122",
		"@types/vscode": "^1.102.0",
		"typescript": "^5.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/palthainon/CiscoHighlighter"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onLanguage:cisco"
	],
	"contributes": {
		"languages": [
			{
				"id": "cisco",
				"aliases": [
					"Cisco",
					"cisco"
				],
				"extensions": [
					".ios"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cisco",
				"scopeName": "text.cisco",
				"path": "./syntaxes/cisco.tmLanguage"
			}
		]
	}
}
